                                                Width="100"
                                                ItemsSource="{Binding TimeWorks}"
                                                SelectedItem="{Binding dtTimeWork}"
                                                DisplayMemberPath="timeWorkName"
                                                SelectedValuePath="timeWorkId"
                                                ></ComboBox>
                                        </StackPanel>
                                        <StackPanel Margin="0 0 30 0" VerticalAlignment="Center">
